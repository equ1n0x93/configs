Host *
  ControlPath ~/.ssh/sockets/%p-%h-%r
  ControlMaster auto
  ControlPersist yes
  ServerAliveInterval 60
  AddKeysToAgent yes
  UseKeyChain yes
  IdentityFile ~/.ssh/id_rsa
  User adahan

Host support-us
  ForwardAgent yes
  ProxyCommand sh -c "nc support-us.lightcyber.com 443 || nc support-us.lightcyber.com 22"

Host support-il
  ForwardAgent yes
  ProxyCommand sh -c "grep -q paloaltonetworks.local /etc/resolv.conf && (nc 10.196.165.51 %p) || (nc support-il.lightcyber.com 443)"

Host support-eu
  ForwardAgent yes
  ProxyCommand sh -c "nc support-eu.lightcyber.com 443 || nc support-eu.lightcyber.com 22"


Host il.*
  User lc
  ProxyCommand ssh support-il $(echo "%h" | cut -d. -f2-)

Host us.*
  User lc
  ProxyCommand ssh support-us $(echo "%h" | cut -d. -f2-)

Host eu.*
  user lc
  ProxyCommand ssh support-eu $(echo "%h" | cut -d. -f2-)

Host kamino
  ForwardAgent yes
  ProxyCommand sh -c "grep -q paloaltonetworks.local /etc/resolv.conf && (nc 10.196.165.12 %p || nc kamino.local %p) || (nc office.lightcyber.com 220)"

Host km.*
  ProxyCommand ssh kamino "nc $(echo %h | cut -d. -f2-) 22"

